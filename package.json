{
  "name": "nu-data",
  "version": "1.2.2",
  "description": "Get NovelUpdate Data",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run -s -w build",
    "dev": "nodemon src/index.js --exec \"node -r dotenv/config -r babel-register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir -p dist && babel src -s -D -d dist",
    "test": "jest --coverage",
    "test-w": "jest --watch",
    "lint": "esw -w src test",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AzSiAz/NU-Data.git"
  },
  "keywords": [
    "NovelUpdate",
    "Novel",
    "Light"
  ],
  "author": "AzSiAz <contact@azsiaz.tech> (azsiaz.tech)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AzSiAz/NU-Data/issues"
  },
  "homepage": "https://github.com/AzSiAz/NU-Data#readme",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-jest": "^19.0.0",
    "babel-preset-env": "^1.2.1",
    "babel-register": "^6.23.0",
    "coveralls": "^2.12.0",
    "dotenv": "^4.0.0",
    "eslint": "^3.17.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-watch": "^3.0.1",
    "jest": "^19.0.2",
    "nodemon": "^1.11.0",
    "supertest": "^3.0.0",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "cheerio": "^0.22.0",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.15.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "plugins": [
      "import",
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "jest": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 0,
      "jest/no-exclusive-tests": 0,
      "jest/no-identical-title": 2
    }
  },
  "jest": {
    "testEnvironment": "node"
  }
}
